@model  IEnumerable<ServiceApp.Domain.DataModel.EngineerInfo>

@{
    ViewBag.Title = "Engineer Information";
}

<!-- BEGIN PAGE LEVEL PLUGINS -->
<link href="~/Assets/global/plugins/datatables/datatables.min.css" rel="stylesheet" type="text/css" />
@*<link href="~/Assets/global/plugins/datatables/plugins/bootstrap/datatables.bootstrap.css" rel="stylesheet" type="text/css" />*@
<!-- END PAGE LEVEL PLUGINS -->
<!-- BEGIN PAGE LEVEL PLUGINS -->
@*<script src="../assets/global/scripts/datatable.js" type="text/javascript"></script>*@
<script src="~/Assets/global/plugins/datatables/datatables.min.js" type="text/javascript"></script>
<script src="~/Assets/global/plugins/datatables/plugins/bootstrap/datatables.bootstrap.js" type="text/javascript"></script>
<script src="~/Assets/global/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
<script src="~/Assets/pages/scripts/ui-modals.min.js" type="text/javascript"></script>
<script src="~/Assets/pages/scripts/modalform.js"></script>
<!-- END PAGE LEVEL PLUGINS -->
<!-- BEGIN PAGE LEVEL SCRIPTS -->
<script src="~/Assets/pages/scripts/table-datatables-buttons.js" type="text/javascript"></script>
<!-- END PAGE LEVEL SCRIPTS -->
<!-- BEGIN Engineer Info-->
<div class="portlet light portlet-fit">
    <div class="portlet-title">
        <div class="caption">
            <i class=" icon-layers font-green"></i>
            <span class="caption-subject font-green bold uppercase">Engineer Information</span>
        </div>
    </div>
    <div class="portlet-body">
        <div class="table-toolbar">
            <div class="row">
                <div class="col-md-6">
                    <div class="btn-group">
                        <a href="@Url.Action("Create", "EngineerInfo")" class="btn green" data-toggle="modal" data-modal="">
                            Add New
                            <i class="fa fa-plus"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        @if (Model != null)
        {
            <table class="table table-striped table-bordered table-hover dataTable no-footer dtr-inline" id="dvAdminEngineerInfo" role="grid" aria-describedby="dvAdminEngineerInfo_info">
                <thead>
                    <tr role="row">
                        <th>Email</th>
                        <th>Name</th>
                        <th>PhoneNumber</th>
                        <th>City</th>
                        <th>State</th>
                        <th>Pincode</th>
                        <th>Membership Type</th>
                        <th>Creation Date</th>
                        <th>Start Date</th>
                        <th>End Date</th>
                        <th>Amount</th>
                        @*<th>Action</th>*@
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr role="row">
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PhoneNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.City)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.State)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Pincode)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MembershipType)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreationDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.StartDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EndDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Amount)
                        </td>
                        @*<td>
                                <div class="btn-group">
                                    <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="1000" data-close-others="true">
                                        <span class="caret"></span>
                                    </button>
                                    <ul class="dropdown-menu" role="menu">
                                        <li>@Html.ActionLink("Details", "Details", new { id = item.Email }, new { data_modal = "" })</li>
                                        <li>@Html.ActionLink("Edit", "Edit", new { id = item.Email }, new { data_modal = "" })</li>
                                        <li class="divider"></li>
                                        <li>@Html.ActionLink("Delete", "Delete", new { id = item.Email }, new { data_modal = "" })</li>
                                    </ul>
                                </div>
                            </td>*@
                    </tr>
                    }
                </tbody>
            </table>
        }
    </div>
</div>
<!-- END Engineer Info-->

<div class="modal fade" id="dvModal" tabindex="-1" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div id='dvModalContent'></div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!-- /.modal -->
