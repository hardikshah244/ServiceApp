<div class="portlet light portlet-fit">
    <div class="portlet-title">
        <div class="caption">
            <i class=" icon-layers font-green"></i>
            <span class="caption-subject font-green bold uppercase">Admin Dashboard</span>
        </div>
    </div>
    <div class="portlet-body">
        <div class="row">
            <div class="col-md-6">
                <!-- BEGIN Portlet PORTLET-->
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption font-green-sharp">
                            <i class="icon-speech font-green-sharp"></i>
                            <span class="caption-subject bold uppercase">Customer Service Requests</span>
                        </div>
                        <div class="actions">
                            <a class="btn btn-circle btn-icon-only btn-default fullscreen" href="javascript:;"> </a>
                        </div>
                    </div>
                    <div class="portlet-body">
                        
                            @if (Model.Item1 != null)
                            {
                                <table class="table table-striped table-bordered table-hover dataTable no-footer dtr-inline" id="dvAdminSRMgtDetails" role="grid" aria-describedby="dvAdminSRMgtDetails_info">
                                    <thead>
                                        <tr>
                                            <th>SR No</th>
                                            <th>Category</th>
                                            <th>Type</th>
                                            <th>Customer Name</th>
                                            <th>Customer PhoneNo</th>
                                            <th>Created DateTime</th>
                                            <th>Assigned DateTime</th>
                                            <th>Completed DateTime</th>
                                            <th>Customer Feedback</th>
                                            <th>Status</th>
                                            <th>Within 99Min</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.Item1)
                                        {
                                            <tr role="row">
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ServiceRequestNO)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ServiceCategoryName)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ServiceTypeName)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.CustName)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.CustPhoneNo)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.CreatedDateTime)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.AssignedDateTime)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.CompletedDateTime)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.CustomerFeedback)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.StatusTypeName)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.WITHIN99)
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            }
                        
                    </div>
